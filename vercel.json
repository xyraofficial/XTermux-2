{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/login",
      "destination": "/api/login.ts"
    },
    {
      "source": "/api/register",
      "destination": "/api/register.ts"
    },
    {
      "source": "/api/chats",
      "destination": "/api/chats.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}